{
  "name": "universidadaustral",
  "version": "1.0.0",
  "description": "This is the global health directory project",
  "main": "index.html",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "lite-server",
    "start": "concurrently \"npm run watch:scss\" \" npm run dev\" ",
    "scss": "node-sass -o css/ css/",
    "watch:scss": "onchange 'css/*.scss' -- npm run scss",
    "clean": "rimraf dist",
    "imagemin": "imagemin img/* --out-dir dist/img",
    "usemin": "usemin index.html -d dist --htmlmin -o dist/index.html && usemin nosotros.html -d dist --htmlmin -o dist/nosotros.html && usemin precios.html -d dist --htmlmin -o dist/precios.html && usemin contacto.html -d dist --htmlmin -o dist/contacto.html",
    "build": "npm run clean && npm run scss && npm run imagemin && npm run usemin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lmunozxyz/UniversidadAustral.git"
  },
  "author": "Luis Andre Munoz, Costa Rica",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lmunozxyz/UniversidadAustral/issues"
  },
  "homepage": "https://github.com/lmunozxyz/UniversidadAustral#readme",
